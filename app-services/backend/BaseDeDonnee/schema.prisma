generator client {
   provider = "prisma-client-js"
}

model categorie {
   id_categorie          Int       @default(autoincrement()) @id
   nom        String
   photo       String
}

model service {
   id_service          Int       @default(autoincrement()) @id
   titre        String
   description      User      @relation(fields: [authorId], references: [id], onDelete: Cascade)
   authorId    Int
}

datasource db {
   provider = "sqlite"
   url      = "file:./dev.db"
}